#: .project:1
msgid "systemback"
msgstr "-"

#: systemback.module:41
msgid "\n Usage: systemback [option]\n\n Options:\n\n  -n, --newbackup          create a new restore point\n\n  -s, --storagedir <path>  get or set restore points storage directory path\n\n  -u, --upgrade            upgrade current system\n                           remove unnecessary files and packages\n\n  -v, --version            output Systemback version number\n\n  -h, --help               show this help\n"
msgstr ""

#: systemback.module:120
msgid "Restore points storage directory is not available or not writable!"
msgstr "Dosarul de salvare nu este accesibil, sau nu este permis scrierea!"

#: systemback.module:157
msgid "First, close all package manager."
msgstr "Închideți toate programele pentru administrare pachete."

#: systemback.module:157
msgid "Unable to get exclusive lock!"
msgstr "Blocarea exclusivă nu este posibilă!"

#: systemback.module:182
msgid "Another Systemback process is currently running, please wait until it\n finishes."
msgstr "Un alt proces Systemback se execută în prezent, vă rugăm să așteptați până\n când se termină."

#: systemback.module:207
msgid "Root privileges are required for running Systemback!"
msgstr "Pentru Systemback sunt necesare privilegii root!"

#: systemback.module:293
msgid "Available restore point(s):"
msgstr "Punct(e) de restabilire accesibil(e):"

#: systemback.module:365
msgid "Create new"
msgstr "Creare nou"

#: systemback.module:365
msgid "Quit"
msgstr "Închide"

