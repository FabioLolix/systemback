    For Each item In Dir(lwdir, "*.sblive", gb.File)

      With Stat(lwdir & "/" & item)

        If Len(item) > 7 And .Size > 0 And .Type <> gb.Link

          If Exist(lwdir & "/" & Left(item, -7) & ".iso")

            With Stat(lwdir & "/" & Left(item, -7) & ".iso")

