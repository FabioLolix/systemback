  With buttonspanel

    If copypanel.Visible = True Or excludepanel.Visible = True Or choosepanel.Visible = True

      With windowmaximize

        If systembackgui.W = Screen.AvailableWidth And systembackgui.H = Screen.AvailableHeight

          If .Text = "" Then .Text = ""

